from prettytable import PrettyTable
table = PrettyTable()
table.add_column("Name",["XYZ","PQR","ABC","YUI"])
table.add_column("Type",["HUMAN","INSAAN","ANIMAL","PARROT"])


#to change the bydefault alignment 
table.align = "l"

print(table) 
